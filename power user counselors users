    <script>
        // Tab functionality
        function showTab(tabName) {
            // Hide all tab contents
            const contents = document.querySelectorAll('.tab-content');
            contents.forEach(content => content.classList.add('hidden'));
            
            // Remove active state from all tabs
            const tabs = document.querySelectorAll('.tab-button');
            tabs.forEach(tab => {
                tab.classList.remove('border-brand-orange-500', 'text-brand-orange-600');
                tab.classList.add('border-transparent', 'text-gray-500');
            });
            
            // Show selected tab content
            document.getElementById(`content-${tabName}`).classList.remove('hidden');
            
            // Add active state to selected tab
            const activeTab = document.getElementById(`tab-${tabName}`);
            activeTab.classList.remove('border-transparent', 'text-gray-500');
            activeTab.classList.add('border-brand-orange-500', 'text-brand-orange-600');
        }

        // Navigation functions
        function showDashboard() {
            alert('Navigating to Dashboard...');
        }

        function showApplicants() {
            alert('Navigating to Applicants page...');
        }

        function showApplications() {
            alert('Navigating to Applications page...');
        }

        function showReports() {
            // Hide users page and show reports page
            document.getElementById('users-page').classList.add('hidden');
            document.getElementById('reports-page').classList.remove('hidden');
            
            // Update navigation highlighting
            const navLinks = document.querySelectorAll('.nav-link');
            navLinks.forEach(link => {
                link.classList.remove('font-bold', 'border-b-2', 'border-brand-orange-500');
                link.classList.add('hover:text-gray-700');
            });
            
            // Highlight Reports nav item
            const reportsLink = document.querySelector('a[onclick="showReports()"]');
            if (reportsLink) {
                reportsLink.classList.add('font-bold', 'border-b-2', 'border-brand-orange-500');
                reportsLink.classList.remove('hover:text-gray-700');
            }
        }

        function showUsers() {
            // Hide reports page and show users page
            document.getElementById('reports-page').classList.add('hidden');
            document.getElementById('users-page').classList.remove('hidden');
            
            // Update navigation highlighting
            const navLinks = document.querySelectorAll('.nav-link');
            navLinks.forEach(link => {
                link.classList.remove('font-bold', 'border-b-2', 'border-brand-orange-500');
                link.classList.add('hover:text-gray-700');
            });
            
            // Highlight Users nav item
            const usersLink = document.querySelector('a[onclick="showUsers()"]');
            if (usersLink) {
                usersLink.classList.add('font-bold', 'border-b-2', 'border-brand-orange-500');
                usersLink.classList.remove('hover:text-gray-700');
            }
        }

        // User Management Functions
        function showAddUserModal() {
            document.getElementById('addUserModal').classList.remove('hidden');
        }

        function closeAddUserModal() {
            document.getElementById('addUserModal').classList.add('hidden');
            document.getElementById('addUserForm').reset();
        }

        function showPermissions(userEmail) {
            // Update modal content based on user
            const userName = userEmail.includes('patricia') ? 'Prof. Patricia Johnson' : 
                           userEmail.includes('michael') ? 'Dr. Michael Chen' :
                           userEmail.includes('sarah') ? 'Sarah Rodriguez' : 'Judge Jennifer Davis';
            
            const userRole = userEmail.includes('patricia') ? 'Administrator' : 
                           userEmail.includes('michael') ? 'Counselor' :
                           userEmail.includes('sarah') ? 'Student' : 'Recommender';
            
            document.getElementById('permissionsUserName').textContent = userName;
            document.getElementById('permissionsUserRole').textContent = userRole;
            
            document.getElementById('permissionsModal').classList.remove('hidden');
        }

        function closePermissionsModal() {
            document.getElementById('permissionsModal').classList.add('hidden');
        }

        function savePermissions() {
            alert('Permissions updated successfully!\n\nChanges saved for user permissions.');
            closePermissionsModal();
        }

        function editUser(userEmail) {
            alert(`Edit User: ${userEmail}\n\nThis would open the user edit form with:\nâ€¢ Personal information\nâ€¢ Role assignment\nâ€¢ Department/organization\nâ€¢ Account status`);
        }

        function deactivateUser(userEmail) {
            if (confirm(`Are you sure you want to deactivate ${userEmail}?\n\nThis will:\nâ€¢ Disable their account access\nâ€¢ Preserve their data\nâ€¢ Allow reactivation later`)) {
                alert(`User ${userEmail} has been deactivated.`);
            }
        }

        function resendInvite(userEmail) {
            alert(`Invitation resent to ${userEmail}\n\nA new invitation email has been sent with:\nâ€¢ Account setup instructions\nâ€¢ Temporary access link\nâ€¢ Role information`);
        }

        function cancelInvite(userEmail) {
            if (confirm(`Cancel invitation for ${userEmail}?\n\nThis will remove them from pending invitations.`)) {
                alert(`Invitation cancelled for ${userEmail}`);
            }
        }

        function bulkActions() {
            const selectedUsers = document.querySelectorAll('.user-checkbox:checked');
            if (selectedUsers.length === 0) {
                alert('Please select users to perform bulk actions.');
                return;
            }
            
            alert(`Bulk Actions Menu\n\nSelected ${selectedUsers.length} users\n\nAvailable actions:\nâ€¢ Activate/Deactivate\nâ€¢ Change roles\nâ€¢ Send notifications\nâ€¢ Export user data\nâ€¢ Delete users`);
        }

        function toggleSelectAll() {
            const selectAll = document.getElementById('selectAll');
            const userCheckboxes = document.querySelectorAll('.user-checkbox');
            
            userCheckboxes.forEach(checkbox => {
                checkbox.checked = selectAll.checked;
            });
        }

        function filterUsers() {
            const searchTerm = document.getElementById('userSearch').value.toLowerCase();
            const roleFilter = document.getElementById('roleFilter').value;
            const statusFilter = document.getElementById('statusFilter').value;
            const userRows = document.querySelectorAll('.user-row');
            
            userRows.forEach(row => {
                const userName = row.querySelector('.text-sm.font-medium.text-gray-900').textContent.toLowerCase();
                const userEmail = row.querySelector('.text-sm.text-gray-500').textContent.toLowerCase();
                const userRole = row.getAttribute('data-role');
                const userStatus = row.getAttribute('data-status');
                
                const matchesSearch = userName.includes(searchTerm) || userEmail.includes(searchTerm);
                const matchesRole = !roleFilter || userRole === roleFilter;
                const matchesStatus = !statusFilter || userStatus === statusFilter;
                
                if (matchesSearch && matchesRole && matchesStatus) {
                    row.style.display = '';
                } else {
                    row.style.display = 'none';
                }
            });
        }

        function exportUsers() {
            alert('Export Users\n\nThis would generate:\nâ€¢ Complete user list with roles\nâ€¢ Permission assignments\nâ€¢ Activity statistics\nâ€¢ Contact information\nâ€¢ Excel/CSV format options');
        }

        function refreshUsers() {
            alert('Refreshing user data...\n\nUpdating:\nâ€¢ User status\nâ€¢ Last activity times\nâ€¢ Permission changes\nâ€¢ New registrations');
        }

        // Add User Form Submission
        document.getElementById('addUserForm').addEventListener('submit', function(e) {
            e.preventDefault();
            
            const firstName = document.getElementById('firstName').value;
            const lastName = document.getElementById('lastName').value;
            const email = document.getElementById('email').value;
            const role = document.getElementById('role').value;
            const department = document.getElementById('department').value;
            const sendInvite = document.getElementById('sendInvite').checked;
            
            alert(`User Added Successfully!\n\nName: ${firstName} ${lastName}\nEmail: ${email}\nRole: ${role}\nDepartment: ${department}\n\n${sendInvite ? 'Invitation email sent!' : 'No invitation sent - manual setup required'}`);
            
            closeAddUserModal();
        });

        // Export functions
        function exportAllReports() {
            alert('Exporting All Reports\n\nThis would generate:\nâ€¢ Comprehensive analytics package\nâ€¢ All charts and tables\nâ€¢ Executive summary report\nâ€¢ Raw data exports\nâ€¢ Formatted for presentation');
        }

        function exportChart(chartType) {
            const chartNames = {
                'trends': 'Application Trends Chart',
                'programs': 'Program Distribution Chart',
                'geography': 'Geographic Distribution Chart',
                'competition': 'Competition Analysis Chart',
                'careers': 'Career Path Outcomes Chart',
                'factors': 'Success Factors Chart',
                'usertypes': 'User Types Chart',
                'activity': 'Activity Trends Chart'
            };
            
            alert(`Exporting ${chartNames[chartType]}\n\nThis would generate:\nâ€¢ High-resolution PNG/PDF\nâ€¢ Interactive web version\nâ€¢ Raw data CSV\nâ€¢ Chart configuration file`);
        }

        function exportTable(tableType) {
            const tableNames = {
                'demographics': 'Student Demographics Table',
                'opportunities': 'Opportunities Analysis Table',
                'alumni': 'Alumni Career Tracking Table',
                'users': 'User Management Table'
            };
            
            alert(`Exporting ${tableNames[tableType]}\n\nThis would generate:\nâ€¢ Excel spreadsheet\nâ€¢ CSV data file\nâ€¢ Formatted PDF report\nâ€¢ Summary statistics`);
        }

        // Mobile menu toggle
        function toggleMobileMenu() {
            const mobileMenu = document.getElementById('mobileMenu');
            if (mobileMenu.classList.contains('hidden')) {
                mobileMenu.classList.remove('hidden');
            } else {
                mobileMenu.classList.add('hidden');
            }
        }

        // Initialize with Applicants tab active
        document.addEventListener('DOMContentLoaded', function() {
            showTab('applicants');
        });
    </script>
